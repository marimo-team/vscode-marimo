{
  "name": "vscode-marimo",
  "displayName": "marimo",
  "author": "marimo team",
  "publisher": "marimo-team",
  "description": "Run marimo in VSCode",
  "packageManager": "pnpm@8.15.1",
  "version": "0.2.3",
  "engines": {
    "vscode": "^1.86.0"
  },
  "private": true,
  "categories": [
    "Notebooks",
    "Machine Learning",
    "Data Science"
  ],
  "icon": "resources/marimo.png",
  "activationEvents": [
    "onLanguage:python",
    "onCommand:vscode-marimo.stop",
    "onCommand:vscode-marimo.edit",
    "onCommand:vscode-marimo.run",
    "onCommand:vscode-marimo.newMarimoFile",
    "onCommand:vscode-marimo.openDocumentation",
    "onCommand:vscode-marimo.openInBrowser",
    "onCommand:vscode-marimo.openOutputsInBrowser",
    "onCommand:vscode-marimo.openOutputsInPanel",
    "onCommand:vscode-marimo.showCommands",
    "onCommand:vscode-marimo.exportAsCommands"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marimo-team/vscode-marimo"
  },
  "bugs": {
    "url": "https://github.com/marimo-team/vscode-marimo/issues"
  },
  "files": [
    "dist",
    "resources"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-marimo.openAsVSCodeNotebook",
        "category": "marimo",
        "enablement": "resourceLangId == python && marimo.isMarimoApp",
        "title": "Start as a native VSCode notebook",
        "shortTitle": "Start as VSCode notebook",
        "icon": "$(notebook)"
      },
      {
        "command": "vscode-marimo.edit",
        "enablement": "resourceLangId == python && marimo.isMarimoApp",
        "category": "marimo",
        "title": "Start in marimo editor (edit)",
        "shortTitle": "Start in marimo editor (edit)"
      },
      {
        "command": "vscode-marimo.run",
        "enablement": "resourceLangId == python && marimo.isMarimoApp",
        "category": "marimo",
        "title": "Start in marimo editor (run)",
        "shortTitle": "Start in marimo editor (run)"
      },
      {
        "command": "vscode-marimo.stop",
        "enablement": "resourceLangId == python && marimo.isMarimoApp",
        "category": "marimo",
        "title": "Stop server"
      },
      {
        "command": "vscode-marimo.restart",
        "enablement": "(resourceLangId == python && marimo.isMarimoApp) || notebookType == marimo-notebook",
        "category": "marimo",
        "title": "Restart kernel",
        "icon": "$(refresh)"
      },
      {
        "command": "vscode-marimo.restartServer",
        "enablement": "(resourceLangId == python && marimo.isMarimoApp) || notebookType == marimo-notebook",
        "category": "marimo",
        "title": "Restart server",
        "icon": "$(refresh)"
      },
      {
        "command": "vscode-marimo.convertToMarimoApp",
        "enablement": "resourceExtname == .ipynb",
        "category": "marimo",
        "title": "Convert Jupyter notebook to marimo notebook",
        "shortTitle": "Convert to marimo notebook",
        "icon": "$(symbol-boolean)"
      },
      {
        "command": "vscode-marimo.newMarimoFile",
        "category": "marimo",
        "title": "Create new marimo file",
        "icon": "$(new-file)"
      },
      {
        "command": "vscode-marimo.newVSCodeNotebook",
        "category": "marimo",
        "title": "Create VSCode notebook",
        "icon": "$(notebook)"
      },
      {
        "command": "vscode-marimo.openDocumentation",
        "category": "marimo",
        "title": "Show documentation doc.marimo.io",
        "shortTitle": "Show documentation",
        "icon": "$(remote-explorer-documentation)"
      },
      {
        "command": "vscode-marimo.openInBrowser",
        "category": "marimo",
        "title": "Open in system browser",
        "icon": "$(link-external)"
      },
      {
        "command": "vscode-marimo.reloadBrowser",
        "enablement": "activeWebviewPanelId == 'marimo'",
        "category": "marimo",
        "title": "Reload browser",
        "icon": "$(refresh)"
      },
      {
        "command": "vscode-marimo.showCommands",
        "enablement": "(resourceLangId == python && marimo.isMarimoApp) || notebookType == marimo-notebook",
        "category": "marimo",
        "title": "Show marimo commands",
        "icon": "$(symbol-event)"
      },
      {
        "command": "vscode-marimo.exportAsCommands",
        "enablement": "(resourceLangId == python && marimo.isMarimoApp) || notebookType == marimo-notebook",
        "category": "marimo",
        "title": "Export notebook as...",
        "icon": "$(export)"
      },
      {
        "command": "marimo-explorer.refresh",
        "enablement": "view == marimo-explorer-applications",
        "category": "marimo",
        "title": "Refresh marimo explorer",
        "icon": "$(refresh)"
      }
    ],
    "notebooks": [
      {
        "type": "marimo-notebook",
        "displayName": "Marimo Notebook",
        "priority": "option",
        "selector": [
          {
            "filenamePattern": "*.py"
          }
        ]
      }
    ],
    "views": {
      "marimo-explorer": [
        {
          "id": "marimo-explorer-applications",
          "name": "Applications",
          "icon": "resources/icon.svg"
        },
        {
          "id": "marimo-explorer-running-applications",
          "name": "Running Applications",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "marimo-explorer",
          "title": "marimo",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "menus": {
      "editor/title": [
        {
          "when": "activeWebviewPanelId == 'marimo'",
          "command": "vscode-marimo.openInBrowser",
          "group": "navigation"
        },
        {
          "when": "activeWebviewPanelId == 'marimo'",
          "command": "vscode-marimo.reloadBrowser",
          "group": "navigation"
        },
        {
          "when": "resourceLangId == python && marimo.isMarimoApp",
          "command": "vscode-marimo.showCommands",
          "group": "navigation"
        },
        {
          "when": "resourceLangId == python && marimo.isMarimoApp",
          "command": "vscode-marimo.exportAsCommands",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .ipynb",
          "command": "vscode-marimo.convertToMarimoApp",
          "icon": "$(symbol-boolean)",
          "group": "navigation"
        }
      ],
      "editor/title/run": [
        {
          "when": "resourceLangId == python && marimo.isMarimoApp",
          "command": "vscode-marimo.edit",
          "group": "navigation"
        },
        {
          "when": "resourceLangId == python && marimo.isMarimoApp",
          "command": "vscode-marimo.run",
          "group": "navigation"
        },
        {
          "when": "resourceLangId == python && marimo.isMarimoApp",
          "command": "vscode-marimo.openAsVSCodeNotebook",
          "icon": "$(notebook)",
          "group": "navigation"
        }
      ],
      "file/newFile": [
        {
          "command": "vscode-marimo.newVSCodeNotebook",
          "group": "navigation"
        }
      ],
      "notebook/toolbar": [
        {
          "when": "notebookType == marimo-notebook",
          "command": "vscode-marimo.restart",
          "group": "navigation/execute@1"
        },
        {
          "when": "notebookType == marimo-notebook",
          "command": "vscode-marimo.showCommands",
          "group": "navigation"
        }
      ],
      "view/title": [
        {
          "command": "marimo-explorer.refresh",
          "when": "view == marimo-explorer-applications",
          "group": "navigation"
        },
        {
          "command": "vscode-marimo.openDocumentation",
          "when": "view == marimo-explorer-applications",
          "group": "navigation"
        },
        {
          "command": "vscode-marimo.newMarimoFile",
          "when": "view == marimo-explorer-applications",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "marimo",
      "properties": {
        "marimo.browserType": {
          "type": "string",
          "enum": [
            "embedded",
            "system"
          ],
          "default": "embedded",
          "description": "Browser to open marimo app"
        },
        "marimo.port": {
          "type": "number",
          "default": 2718,
          "description": "Default port for marimo server"
        },
        "marimo.enableToken": {
          "type": "boolean",
          "default": false,
          "description": "Enable token authentication"
        },
        "marimo.tokenPassword": {
          "type": "string",
          "default": "",
          "description": "Password for token authentication"
        },
        "marimo.showTerminal": {
          "type": "boolean",
          "default": false,
          "description": "Open the terminal when the server starts"
        },
        "marimo.debug": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug mode"
        },
        "marimo.pythonPath": {
          "type": "string",
          "default": "",
          "description": "Path to python executable"
        },
        "marimo.marimoPath": {
          "type": "string",
          "default": "marimo",
          "description": "Path to marimo executable"
        }
      }
    }
  },
  "scripts": {
    "build": "tsup src/extension.ts --external=vscode",
    "dev": "pnpm run build --watch",
    "lint": "biome check --apply .",
    "pack": "vsce package --no-dependencies",
    "codegen": "npx openapi-typescript https://raw.githubusercontent.com/marimo-team/marimo/0.6.22/openapi/api.yaml --immutable -o ./src/generated/api.ts",
    "pretest": "pnpm run build && pnpm run lint",
    "publish": "vsce publish --no-dependencies",
    "release": "npx bumpp && pnpm run publish",
    "test": "echo 'No tests yet...'",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "pnpm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "20.12.12",
    "@types/vscode": "^1.86.0",
    "@vscode/test-electron": "^2.3.10",
    "openapi-typescript": "^7.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@jupyterlab/nbformat": "^4.2.1",
    "@types/ws": "^8.5.10",
    "jsdom": "^24.1.0",
    "node-fetch": "^3.3.2",
    "openapi-fetch": "^0.9.7",
    "tempy": "^3.1.0",
    "ws": "^8.17.0"
  }
}
